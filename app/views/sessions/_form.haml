#signin
  %fieldset
    %legend
      Sign in
    %table.user-table
      %tr.field
        %td.label= f.label :email 
        %td.input= f.text_field :email
      %tr.field
        %td.label= f.label :password
        %td.input= f.password_field :password
      %tr.field
        %td.label= f.label :password_confirmation
        %td.input= f.password_field :password_confirmation
      %tr.field
        %td.label= f.label :remember_me, 'Remember me on this computer'
        %td.input= f.check_box :remember_me