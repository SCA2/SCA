<fieldset id="payment">
  <legend><%= legend %></legend>
  <table class="address-table">
    <tr class="field">
      <td class="label"><%= label_tag 'card-element', "Credit or Debit Card" %></td>
      <td class="input" id="card-element" data-postcode=<%= f.object.postcode %>></td>
    </tr>
    <tr class="field">
      <td></td>
      <td id="card-errors"></td>
    </tr>
    <tr class="field">
      <td class="label"><%= label_tag :email %></td>
      <td class="input"><%= f.text_field :email %></td>
    </tr>
    <%= f.hidden_field :stripe_token %>
 	</table>
</fieldset>
