= provide(:title, "New Component")
.box_2
	.row
		.small-12.columns
			= form_for component do |f|
				= render 'shared/errors', object: f.object
				= render 'form', f: f, legend: "New Component"
				.admin-box
					= f.submit "Update", class: "button admin"
					= link_to "Quit", components_path, class: "button admin"