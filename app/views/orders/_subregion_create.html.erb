<% address_type = f.object.address_type %>
<div id=<%= "order_state_code_wrapper_" + address_type %>>
	<% parent_region = 'US' %>
  <% country = Carmen::Country.coded(parent_region) %>
	 
  <% if country.nil? %>
    <em>Please select a country below</em>
  <% elsif country.subregions? %>
  	<%= logger.debug "country.subregions: " + country.subregions.inspect %>
    <%= subregion_select(:order, :state_code, parent_region) %>
  <% else %>
    <%= text_field(:order, :state_code) %>
  <% end %>
</div>