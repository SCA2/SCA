<% provide(:title, 'All users') %>

<div class="box_2">
	<div class="row">
		<div class="small-3 columns" style="margin: 1em 0em 1em 1em">
			<%= render 'layouts/sidebar_left', :products => @products %>
		</div>
		<div class="small-8 columns" style="margin: 1em 1em 0em 0em">
			<div class="section_header">All Users</div>
			<%= will_paginate %>
			<ul class="users">
				<%= render @users %>
			</ul>
			<%= will_paginate %>
		</div>
	</div>	
</div> <!-- box_2 -->
